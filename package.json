{
  "name": "react-tailwind-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.7",
    "autoprefixer": "^10.4.2",
    "immer": "^9.0.12",
    "immutable": "^4.0.0",
    "postcss-cli": "^9.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "tailwindcss": "^3.0.12",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "build:css": "cross-env NODE_ENV=production postcss src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "build:css-dev": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "start:js": "react-scripts start",
    "start": "npm-run-all build:css-dev start:js",
    "build:js": "react-scripts build",
    "build": "npm-run-all build:css build:js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4"
  }
}
